# Environment variables for local development
# Copy this file to .env and fill in your values

# Your MCP secret token (required)
MCP_SECRET=your-secret-token-here

# GCP Configuration (optional - uses gcloud defaults if not set)
GCP_PROJECT_ID=
REGION=us-central1
SERVICE_NAME=mcp-fresh-server
REPOSITORY=mcp

# For GitHub Actions (if using)
GCP_SA_KEY=your-service-account-key-json

# Local development
PORT=8080
GOOGLE_CLOUD_PROJECT=
